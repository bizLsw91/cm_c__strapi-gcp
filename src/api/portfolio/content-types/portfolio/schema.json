{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "singularName": "portfolio",
    "pluralName": "portfolios",
    "displayName": "포트폴리오",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "year": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "min": 2010,
      "default": 2010
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "institution": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "outline": {
      "displayName": "outline",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.outline"
    },
    "category": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "pubGov",
        "performance",
        "event"
      ],
      "default": "pubGov"
    },
    "title_en_translate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false
    },
    "startDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^\\d{4}(-\\d{2}){0,2}$",
      "default": "ex) YYYY or YYYY-MM or YYYY-MM-DD"
    },
    "endDate": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "default": "ex) YYYY or YYYY-MM or YYYY-MM-DD",
      "regex": "^\\d{4}(-\\d{2}){0,2}$"
    }
  }
}
